{
  "raw-content": {
    "configuration": {
      "analyzer": {
        "bundle": "raw-content2",
        "name": "standard"
      },
      "math": {
        "bundle": "math2",
        "name": "standard"
      }
    },
    "classification": {
      "types": [ "text", "shape", "math", "drawing" ]
    },
    "recognition": {
      "types": [ "text", "shape", "math" ]
    },
    "convert": {
      "shape-on-hold": true
    },
    "shape": {
      "snap-axis": [ ]
    },
    "eraser": {
      "erase-precisely": false,
      "dynamic-radius": true
    },
    "auto-connection": false,
    "edge": {
      "policy": [ ]
    },
    "interactive-blocks": {
      "auto-classified": [ "text", "math", "shape" ],
      "feedback": [ "text", "math" ],
      "feedback-hints": [ "text", "math" ]
    },
    "pen": {
      "gestures": [ "scratch-out", "strike-through" ]
    },
    "line-pattern": "grid",
    "guides": {
      "show": [ "alignment", "text", "square", "square-inside", "image-aspect-ratio", "rotation" ],
      "snap": [ "alignment", "text", "square", "square-inside", "image-aspect-ratio", "rotation" ]
    }
  },
  "convert": {
    "convert-on-double-tap": false
  },
  "math": {
      "solver": {
          "options": "algebraic",
          "numerical-computation": [ "at-right-of-equal-sign", "at-question-mark" ],
          "enable-syntactic-correction": false,
          "display-implicit-multiply": false,
          "auto-variable-management": {
              "enable": true,
              "scoping-policy": "closest"
          }
      }
  }
}
